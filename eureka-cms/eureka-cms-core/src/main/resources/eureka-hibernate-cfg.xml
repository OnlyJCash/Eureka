<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
		<property name="show_sql">false</property>
		<property name="format_sql">false</property>
		<property name="use_outer_join">false</property>

		<mapping class="com.eureka.cms.core.data.model.Audit" />
		<mapping class="com.eureka.cms.core.data.model.EurekaUser" />
		<mapping class="com.eureka.cms.core.data.model.Group" />
		<mapping class="com.eureka.cms.core.data.model.SessionToken"/>
		
	</session-factory>
</hibernate-configuration>